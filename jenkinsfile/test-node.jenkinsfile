node() {
  def workspace = pwd()
  def powershellFile = "\"${workspace}\\scripts\\posh.ps1\""

  stage('Call Powershell') {
    def result = powershell (
      script: powershellFile,
      returnStdout: true
    ).trim()

    println result
  }

  stage('Embedded powershell') {
    powershell '''
      write-error "Fail fail fail"
    '''

  }
}